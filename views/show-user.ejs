<%- include('./partials/header.ejs') %>

<div style="height: 6rem; width: 100%; background: none;"></div>


<h1>show page for <%= user.username %></h1>
<div>admin: <%= user.isAdmin %></div>
<div>cgpa: <%= user.cgpa %></div>
